<body>
<style type="text/css">
    body { background: black; }
</style>

<div class="container" ng-app="MyApp">
    <div class="row">
        <div class="col-xsm-2 col-md-4">
            <form class="form-signin" ng-controller="Register as reg" ng-submit="reg.createAccount()">
                <h2 class="form-signin-heading"><FONT COLOR="white">Campus Connection</FONT></h2>
                <label for="inputFName" class="sr-only" >First Name</label>
                <input type="text" id="inputFName" ng-model="reg.fName" class="form-control" placeholder="First Name" required autofocus>
                <label for="inputLName" class="sr-only" >Last Name</label>
                <input type="text" id="inputLName" class="form-control" ng-model="reg.lName" placeholder="Last Name" required>
                <label for="inputEmail" class="sr-only" >Username</label>
                <input type="text" id="inputEmail" class="form-control" ng-model="reg.username" placeholder="Username" required>
                <label for="inputEmail" class="sr-only" >Email address</label>
                <input type="email" id="inputEmail" class="form-control" ng-model="reg.email" placeholder="Email address" required>
                <label for="inputPassword1" class="sr-only" >Password</label>
                <input type="password" id="inputPassword1" class="form-control" ng-model="reg.password1" placeholder="Password" required>
                <input type="password" id="inputPassword2" class="form-control" ng-model="reg.password2" placeholder="Repeat Password" required>
                <button class="btn btn-lg btn-primary btn-block" type="submit">Register</button>
            </form>
        </div>

        <div class="col-md-6 col-md-6">

            <p>
            <div class="thumbnail">
                <img src="http://img12.deviantart.net/2f7e/i/2010/099/7/a/sjsu_wallpaper_by_spader725.jpg" alt="...">
            </div>
            </p>
        </div>

    </div>
</div> <!-- /container -->


